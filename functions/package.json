{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deployfetchLolChampion": "firebase deploy --only functions:fetchLolChampion",
    "deployfetchLolChampions": "firebase deploy --only functions:fetchLolChampions",
    "deploydialogflowFirebaseFulfillment": "firebase deploy --only functions:dialogflowFirebaseFulfillment",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "main": "index.js",
  "dependencies": {
    "@google-cloud/pubsub": "^2.5.0",
    "actions-on-google": "^2.13.0",
    "cheerio": "^1.0.0-rc.3",
    "dialogflow-fulfillment": "^0.6.1",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}